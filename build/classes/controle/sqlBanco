CREATE TABLE `bancariodb`.`clientesJ` ( 
`idPessoa` INT(11) NOT NULL , 
`nomePessoa` VARCHAR(50) NOT NULL ,
 `nomeSocial` VARCHAR(20) NOT NULL ,
  `cnpj` VARCHAR(14) NOT NULL , 
  `cep` VARCHAR(8) NOT NULL , 
  `num` VARCHAR(10) NOT NULL , 
  `complemento` VARCHAR(25) NOT NULL 
  ) ENGINE = InnoDB;
  
  CREATE TABLE `bancariodb`.`clientesF` ( 
`idPessoa` INT(11) NOT NULL , 
`nomePessoa` VARCHAR(50) NOT NULL ,
 `nomeSocial` VARCHAR(20) NOT NULL ,
  `cpf` VARCHAR(11) NOT NULL , 
  `cep` VARCHAR(8) NOT NULL , 
  `num` VARCHAR(10) NOT NULL , 
  `complemento` VARCHAR(25) NOT NULL 
  ) ENGINE = InnoDB;
  
   CREATE TABLE `bancariodb`.`contaC` ( 
`numConta` INT PRIMERY KEY NOT NULL , 
`cliente` VARCHAR(50) NOT NULL ,
 `saldo` FLOAT NOT NULL ,
  `senha` VARCHAR(11) NOT NULL  
  ) ENGINE = InnoDB;
  
     CREATE TABLE `bancariodb`.`contaC` ( 
`numConta` INT PRIMARY KEY NOT NULL , 
`cliente` VARCHAR(50) NOT NULL ,
 `saldo` VARCHAR(20) NOT NULL ,
  `senha` VARCHAR(11) NOT NULL ,
  `limite`FLOAT NOT NULL 
  ) ENGINE = InnoDB;